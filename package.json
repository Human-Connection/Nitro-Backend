    "@babel/register": "^7.0.0",
    "apollo-boost": "^0.1.10",
    "apollo-cache-inmemory": "^1.3.11",
    "apollo-client": "^2.3.2",
    "apollo-link-http": "^1.5.4",
    "apollo-server": "^2.0.4",
    "bcryptjs": "^2.4.3",
